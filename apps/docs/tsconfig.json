{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "tsconfig/vitepress.json",
  "compilerOptions": {
    "baseUrl": ".",
    "types": [
      "@types/node",
      "vite-plugin-pwa/client"
    ]
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.d.ts",
    ".vitepress/**/*.ts",
    ".vitepress/**/*.vue"
  ],
  "exclude": [
    "cache",
    "dist",
    "node_modules"
  ]
}