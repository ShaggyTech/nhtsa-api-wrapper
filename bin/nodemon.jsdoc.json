{
  "restartable": "rs",
  "ignore": [
    ".git",
    "bin/**/*",
    "dev/**/*",
    "dist/**/*",
    "docs/**/*",
    "node_modules/**/*"
  ],
  "verbose": false,
  "watch": ["src", "docsSrc", "jsdoc.js", "README.md"],
