{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "**/.vitepress/dist/**"]
    },
    "prepare": {
      "outputs": ["dist/**"]
    },
    "lint": {
      "outputs": []
    },
    "lint:fix": {
      "outputs": []
    },
    "format": {
      "cache": false,
      "dependsOn": ["^lint"]
    },
    "dev": {
      "cache": false
    },
    "docs#dev": {
      "dependsOn": ["@shaggytools/nhtsa-api-wrapper#prepare"]
    },
    "test": {
      "outputs": []
    }
  }
}
