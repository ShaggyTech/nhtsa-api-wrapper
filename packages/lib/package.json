{
  "name": "@shaggytools/nhtsa-api-wrapper",
  "version": "3.0.0",
  "author": "Brandon Eichler <github.com/shaggytech>",
  "description": "Universal javascript wrapper for the NHTSA.dot.gov VPIC 'vehicles' API, useful for VIN decoding, etc.",
  "homepage": "https://www.shaggytech.com/nhtsa-api-wrapper",
  "license": "MIT",
  "libraryName": "nhtsa-api-wrapper",
  "private": false,
  "sideEffects": false,
  "main": "dist/nhtsa-api-wrapper.umd.js",
  "module": "dist/nhtsa-api-wrapper.mjs",
  "types": "dist/types/index.d.ts",
  "unpkg": "dist/nhtsa-api-wrapper.iife.js",
  "jsdelivr": "dist/nhtsa-api-wrapper.iife.js",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/nhtsa-api-wrapper.mjs",
      "require": "./dist/nhtsa-api-wrapper.umd.js"
    },
    "./dist/*": "./dist/*",
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "packageManager": "pnpm@7.25.1",
  "engines": {
    "node": ">=16.13.0",
    "pnpm": ">=7.27.1"
  },
  "workspaces": [
    "apps/*",
    "config/*",
    "packages/*"
  ],
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "pnpm run \"/^build:.*/\"",
    "build:lib": "tsc --noEmit && vite build",
    "build:typedocs": "typedoc --options .typedoc/typedoc.js",
    "dev": "pnpm run \"/^dev:.*/\"",
    "dev:lib": "vite build --watch --outDir dev/dist",
    "dev:typedocs": "typedoc --options .typedoc/typedoc.js --watch",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "eslint": "8.32.0",
    "eslint-config-custom": "workspace:*",
    "prettier": "2.8.3",
    "prettier-config": "workspace:*",
    "rimraf": "4.1.1",
    "tsconfig": "workspace:*",
    "typedoc": "0.23.24",
    "typedoc-config": "workspace:*",
    "typescript": "4.9.4",
    "vite": "4.0.4",
    "vite-plugin-dts": "1.7.1",
    "vite-tsconfig-paths": "4.0.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ShaggyTech/nhtsa-api-wrapper.git",
    "homepage": "https://github.com/ShaggyTech/nhtsa-api-wrapper"
  },
  "bugs": {
    "url": "https://github.com/ShaggyTech/nhtsa-api-wrapper/issues"
  },
  "keywords": [
    "NHTSA",
    "NHTSA.gov",
    "NHTSA.dot.gov",
    "NHTSA API",
    "NHTSA API Wrapper",
    "NHTSA API Wrapper for Node",
    "NHTSA API Wrapper for the browser",
    "National Highway Traffic Safety Administration",
    "Typescript",
    "Vehicle",
    "Vehicle API",
    "Vehicle Identification Number",
    "Vehicle Identification Number API",
    "VIN",
    "VIN decode",
    "VIN decoder",
    "VPIC"
  ],
  "publishConfig": {
    "access": "public"
  }
}
