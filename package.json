{
  "name": "@shaggytools/vin-decoder",
  "version": "0.1.0",
  "description": "A package to verify and decode Vehicle Indentification Numbers (VIN) using the NHTSA.gov public API",
  "engines": {
    "node": ">=8.10.0"
  },
  "main": "src/main.js",
  "scripts": {
    "test": "jest --watchAll",
    "test:clearCache": "./node_modules/.bin/jest --clearCache",
    "lint": "eslint --ext .js --ignore-path .gitignore .",
    "lint:fix": "eslint --fix --ext .js --ignore-path .gitignore .",
    "docs:build": "jsdoc -c ./jsdoc.json",
    "docs:watch": "npx nodemon --exec jsdoc -c ./jsdoc.json --ext js,json --watch src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ShaggyTech/vin-decoder-js.git"
  },
  "keywords": [
    "VIN",
    "Vehicle",
    "Identification",
    "Number",
    "NHTSA.gov",
    "NHTSA",
    "decode",
    "decoder"
  ],
  "author": "Brandon Eichler <ShaggyTech>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ShaggyTech/vin-decoder-js/issues"
  },
  "homepage": "https://github.com/ShaggyTech/vin-decoder-js",
  "devDependencies": {
    "@babel/core": "7.7.5",
    "@babel/preset-env": "7.7.6",
    "babel-eslint": "^10.0.3",
    "babel-jest": "24.9.0",
    "better-docs": "^1.4.7",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "jest": "24.9.0",
    "jsdoc": "^3.6.3",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "rollup": "1.27.9"
  },
  "dependencies": {
    "axios": "0.19.0",
    "dotenv": "^8.2.0",
    "eslint-plugin-jsdoc": "^18.4.3"
  }
}
