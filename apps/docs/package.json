{
  "name": "docs",
  "private": true,
  "version": "0.0.1",
  "description": "Documentation for @shaggytools/nhtsa-api-wrapper",
  "main": "index.js",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "vitepress dev src --host",
    "build": "vitepress build src",
    "preview": "vitepress preview src",
    "lint": "eslint .",
    "lint:fix": "eslint --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,md}\"",
    "format:typedoc": "prettier --write \"src/typedoc/**/*.md\"",
    "clean": "rimraf src/.vitepress/dist src/.vitepress/cache .turbo",
    "clean:all": "pnpm run clean && rimraf node_modules"
  },
  "dependencies": {
    "@shaggytools/nhtsa-api-wrapper": "workspace:*",
    "vue": "3.2.47"
  },
  "devDependencies": {
    "eslint": "8.37.0",
    "eslint-config-custom": "workspace:*",
    "prettier": "2.8.7",
    "prettier-config": "workspace:*",
    "rimraf": "4.4.1",
    "tsconfig": "workspace:*",
    "typescript": "5.0.3",
    "vitepress": "1.0.0-alpha.64"
  },
  "eslintConfig": {
    "extends": [
      "custom"
    ],
    "root": true
  },
  "prettier": "prettier-config"
}
