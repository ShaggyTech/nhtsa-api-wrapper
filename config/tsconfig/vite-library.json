{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Vite-Library-Mode",
  "extends": "./base.json",
  "compilerOptions": {
    "allowJs": true,
    "lib": ["ESNext", "DOM"],
    "module": "ESNext",
    "noImplicitReturns": true,
    "resolveJsonModule": true,
    "target": "ESNext",
    "useDefineForClassFields": true
  }
}
